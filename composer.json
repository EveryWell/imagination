{
    "name": "everywell/imagination",
    "description": "A Laravel package that allows to handle images on your models seamlessly",
    "keywords": [
        "images",
        "models",
        "laravel",
        "cache"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Andrea Ognibene",
            "email": "aneverywell@gmail.com"
        }
    ],
    "require": {
        "php": "^5.5.9 || ^7.0",
        "intervention/image": "^2.3",
        "illuminate/support": "^5.4",
        "illuminate/bus": "^5.4",
        "illuminate/queue": "^5.4"
    },
    "autoload": {
        "psr-4": {
            "EveryWell\\Imagination\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
